<p id="notice"><%= notice %></p>
<div class = "title-container">
  <h1>Manutenções</h1>
</div>
  <ul>
    <%= form_tag manutencaos_path, :method => 'get' do %>
      <%= text_field_tag :search, params[:search] %>
      <%= submit_tag "Buscando", :value => "Buscar" %>
    <% end %>
  </ul>

  <% @manutencaos.each do |manutencao| %>
    <ul>
      <li>
        <%= link_to manutencao.realizadoQuando, manutencao_path(manutencao), class: "btn btn-default" %>
        <%= link_to "Desagendar", manutencao, data: {:confirm => "Deseja remover o usuário?"}, :method => :delete, class: "btn btn-default" %>
      </li>
    </ul>
  <% end %>
<br>
<table>
  <tr>
    <th>manutencao de óleo e filtro de óleo</th>

  </tr>

  <% @Vmanutencao.each do |manutencao|  %>
        <tr>
          <td><%= manutencao.placa %></td>
        </tr>
        <% end %>

</table>

<table>
  <tr>
    <th>manutencao de filro de combustível e de ar </th>

  </tr>

  <% @Vmanutencao20.each do |manutencao|  %>
    <tr>
      <td><%= manutencao.placa %></td>
    </tr>
  <% end %>

</table>

<table>
  <tr>
    <th>manutencao de correia dentada</th>

  </tr>

  <% @Vmanutencao40.each do |manutencao|  %>
    <tr>
      <td><%= manutencao.placa %></td>
    </tr>
  <% end %>
</table>

<table>
  <tr>
    <th>manutencao de velas </th>

  </tr>

  <% @VmanutencaoVela.each do |manutencao|  %>
    <tr>
      <td><%= manutencao.placa %></td>
    </tr>
  <% end %>
</table>
<%= link_to 'Agendar Manutenção', new_manutencao_path, class:"btn btn-default" %>
